<body id="page-top" style="background-color: #e4e4e4;">
    <div id="wrapper">

        <div class="d-flex flex-column" id="content-wrapper">
            <div id="content">


                {{>header}}

                <div class="container-fluid">



                    <div class="row" style="margin-top: 30px;">

                        <div class="col-lg-4 col-xl-4">


                            <div class="card shadow mb-4">

                                <div class="card-header d-flex justify-content-between align-items-center">
                                    <h6 class="text-primary fw-bold m-0">My Contests </h6>

                                </div>

                                <div class="card-body" style="    min-height: 400px;">
                                    <div
                                        style="text-align: center; padding-top: 25px ;font-size: 2rem;    border-radius: 26px; ">
                                        <strong> My Current Contests</strong>
                                    </div>
                                    <div class="col text-center">
                                        {{#each cclist2}}
                                        <div class="table-responsive table-wrapper-scroll-y my-custom-scrollbar">

                                            <table class="table table-dark table-striped" id="example" 
                                                <thead>
                                                <tr>

                                                    <th colspan="6">
                                                        {{this.contestName}}
                                                    </th>

                                                </tr>
                                                <tr>
                                                    <th>
                                                        Rank
                                                    </th>
                                                    <th>
                                                        Username
                                                    </th>
                                                    <th>
                                                        Invested Amount
                                                    </th>
                                                    <th>
                                                        Available Amount
                                                    </th>
                                                    <th>
                                                        Booked Profit
                                                    </th>
                                                    <th>
                                                        In profit
                                                    </th>
                                                </tr>
                                                </thead>

                                                <tbody id="tbody">
                                                    <!-- Multiple contests rows for users -->
                                                    {{#each this.participants}}
                                                    <tr class="text">
                                                        <td >{{@index }} </td>
                                                        <td >
                                                            <input
                                                                style="height: 36px; align-items: center; text-align: center; justify-content: center; color: white; "
                                                                type="text" readonly class="form-control-plaintext"
                                                                id="staticEmail" name="bcs" value={{this.username}}>
                                                        </td>
                                                        <td >
                                                            <input
                                                                style="height: 36px; align-items: center; text-align: center; justify-content: center; color: white; "
                                                                type="text" readonly class="form-control-plaintext"
                                                                id="staticEmail" name="bcs"
                                                                value={{this.invested_amount}}>
                                                        </td>

                                                        <td ><input
                                                                style="height: 36px; align-items: center; text-align: center; justify-content: center; color: white; "
                                                                type="text" readonly class="form-control-plaintext"
                                                                id="staticEmail" name="bcs"
                                                                value={{this.available_amount}}>
                                                        </td>
                                                        <td ><input
                                                                style="height: 36px; align-items: center; text-align: center; justify-content: center; color: white; "
                                                                type="text" readonly class="form-control-plaintext"
                                                                id="staticEmail" name="bcs" value={{this.Profit}}></td>
                                                        <td >
                                                            <input
                                                                style="height: 36px; align-items: center; text-align: center; justify-content: center; color: white; "
                                                                type="text" readonly class="form-control-plaintext"
                                                                id="staticEmail" name="bcs" value={{this.inProfit}}>
                                                        </td>
                                                    </tr>
                                                    {{/each}}

                                                </tbody>

                                                <tfoot>
                                                    <tr>
                                                        <td colspan="3" class="text-center">Add users&nbsp;<br>{send req
                                                            to add}</td>
                                                        <td colspan="3" class="text-center">

                                                            <form action="/competition/sendContestReq" method="POST">
                                                                <input type="text"
                                                                    style="border-radius: 9px;margin-right: 7px;"
                                                                    name="searchUname" placeholder="search username" ,>

                                                                <button class="btn btn-info" name="cnameee"
                                                                    value="{{this.contestName}}"
                                                                    type="submit">Send</button>
                                                            </form>

                                                        </td>
                                                    </tr>
                                                </tfoot>

                                            </table>
                                        </div>
                                        <hr>
                                        <hr>
                                        {{/each}}

                                    </div>
                                </div>
                            </div>

                        </div>


