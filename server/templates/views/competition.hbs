                                                            <tr>
                                                                <th class="text-center">Create New Contets</th>
                                                                <th class="text-center">Admin : - {{admin}} <br><br>
                                                                </th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <form action="/competition/createcontest" method="POST">
                                                                <tr>
                                                                    <td class="text-center">Contests Name</td>
                                                                    <td class="text-center">
                                                                        <input type="text" style="border-radius: 9px;"
                                                                            name="contest_name"
                                                                            placeholder="contest_name" ,
                                                                            value={{contest_name}}>
                                                                    </td>
                                                                </tr>

                                                                <tr>
                                                                    <td class="text-center"><button
                                                                            class="btn btn-danger"
                                                                            type="submit">Cancel</button></td>
                                                                    <td class="text-center"><button
                                                                            class="btn btn-primary"
                                                                            type="submit">Create</button></td>
                                                                </tr>
                                                            </form>

                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                </div>
                            </div>
                        </div>




                        <div class="col-lg-7 col-xl-4">
                            <div class="row">
                                <div class="card shadow mb-4">
                                    <div class="card-header d-flex justify-content-between align-items-center">
