                                                <tr>
                                                    <th>
                                                        Contest Name
                                                    </th>
                                                    <th>
                                                        From user
                                                    </th>

                                                    <th>
                                                        accept
                                                    </th>
                                                    <th>
                                                        Decline
                                                    </th>

                                                </tr>
                                                </thead>

                                                <tbody id="tbody">
                                                    <!-- Multiple contests rows for users -->
                                                    {{#each ccrequests}}
                                                    <tr class="text">
                                                        <td >{{this}} </td>
                                                        <td >username </td>

                                                        <td >
                                                            <div>
                                                                <form action="/competition/acceptContestReq" method="post">

                                                                    <button class="btn btn-success btn-lg" type="submit"
                                                                        name="incoming_req_contest_name" style="    margin: 0px;
                                                    margin-bottom: 12px;padding: 2px 8px;font-size: 15px;"
                                                                        value={{this}}>
                                                                        Accept
                                                                    </button>
                                                                </form>

                                                            </div>
                                                        </td>
                                                        <td >
                                                            <div>
                                                                <form action="/competition/declineContestReq" method="post">

                                                                    <button class="btn btn-success btn-lg" type="submit"
                                                                        name="incoming_req_contest_name" style="    margin: 0px;
                                                    margin-bottom: 12px;padding: 2px 8px;font-size: 15px;"
                                                                        value={{this}}>
                                                                        Decline
                                                                    </button>
                                                                </form>

                                                            </div>
                                                        </td>

                                                    </tr>
                                                    {{/each}}
                                                </tbody>
                                            </table>
                                        </div>




                                    </div>
                                </div>









                                <div class="card shadow mb-4">
                                    <div class="card-header d-flex justify-content-between align-items-center">
